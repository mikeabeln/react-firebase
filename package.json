{
  "name": "webpack-react-build",
  "engines": {
    "node": "6.11.5"
  },
  "version": "1.0.0",
  "description": "A starter project using: React, Webpack, Firebase + an Express REST API on a Cloud Function.",
  "private": true,
  "scripts": {
    "lint": "eslint --ignore-path .gitignore client/**.jsx",
    "dev": "yarn lint && webpack-dev-server --open --hot --config webpack.dev.js",
    "prebuild": "rimraf server/public && yarn lint",
    "build": "webpack --progress --config webpack.dist.js",
    "deploy": "cd server && firebase deploy && cd .."
  },
  "keywords": [],
  "author": "Mike Abeln",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-hot-loader": "^1.4.0",
    "css-loader": "^0.28.11",
    "cssnano": "^4.0.2",
    "csv-loader": "^2.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.0",
    "normalize.css": "^8.0.0",
    "postcss-loader": "^2.1.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "hoek": "^4.2.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^4.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2"
  }
}
